ALGORITHM PROBLEM 2 
VAR
    i,j,n:INTEGER;
    tab1,tab2: ARRAY_OF INTEGER[n];
    sum:INTEGER:=0;
    k:BOOLEAN:=true;
BEGIN
    FOR i FROM 0 TO tab1.length -1  DO
        FOR j FROM 0 TO tab2.length  -1 DO 
        
            IF { ( tab1[i] = tab2[j]) THEN
                sum:= sum + (tab1[i] *2 ) ;
}
            END_IF
    END_FOR

END_FOR


 write(sum);

    
END