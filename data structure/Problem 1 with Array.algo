ALGORITHM Problem 1 with Array
VAR
    tab1 ,tab2, : ARRAY_OF INTEGER[N];
    i,j,N :INTEGER;
    k:BOOLEAN :=false;
    sum:INTEGER:=0;
BEGIN
 FOR i FROM 0 TO tab1.length   DO
     FOR j FROM 0 TO tab2.length   DO
         
IF  (tab1[i] = tab2[j]) THEN
 k:=true;
END_IF
END_FOR 
IF (NOT k) THEN
    sum:= sum + tab1[i] ;
END_IF

END_FOR 



    
END